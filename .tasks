# vim: set fenc=utf-8 ft=taskini:
# see: https://github.com/skywind3000/asynctasks.vim/wiki/Task-Config

[project-init]
command=echo "init"

[project-build]
command=go build
cwd=<root>

[project-run]
command=$(VIM_ROOT)/$(VIM_PRONAME)
command/win32=$(VIM_PRONAME)
cwd=<root>
output=terminal

[project-test]
command=./test_one
command/win32=test_one
cwd=<root>/tests
output=terminal


